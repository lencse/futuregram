{
  "name": "futuregram",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:lencse/futuregram.git",
  "author": "Levente LÃ¶ki <leventeloki@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build-ci": "rm -rf ci/bin && tsc -p ./ci/src --outDir ./ci/bin",
    "write-db-file": "node ci/bin/build-database.js",
    "clean": "rm -rf build dist",
    "build-typescript": "tsc -p ./src --outDir ./build",
    "build-webpack": "webpack --colors --progress --config ./ci/bin/webpack.config.js",
    "build": "yarn run clean && yarn build-typescript && yarn build-webpack",
    "watch-typescript": "tsc -p ./src --outDir ./build --watch",
    "watch-webpack": "webpack-dev-server --config ./ci/bin/webpack.config.js --port 8400",
    "watch": "yarn run clean && (yarn watch-typescript & yarn watch-webpack)",
    "postinstall": "yarn build-ci && yarn write-db-file",
    "test": "if [ ! -f ./dist/index.html ]; then echo \"index.html is missing\"; exit 1; fi; if [ ! -d ./dist/js ]; then echo \"dist/js is missing\"; exit 1; fi; if [ ! -d ./dist/css ]; then echo \"dist/css is missing\"; exit 1; fi; if [ ! -d ./dist/images ]; then echo \"dist/images is missing\"; exit 1; fi; find -L dist"
  },
  "devDependencies": {
    "@types/colors": "^1.1.3",
    "@types/command-line-args": "^4.0.1",
    "@types/core-js": "^0.9.43",
    "@types/lodash": "^4.14.85",
    "@types/node": "^8.0.51",
    "@types/react": "^16.0.22",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "colors": "^1.1.2",
    "command-line-args": "^4.0.7",
    "copy-webpack-plugin": "^4.2.0",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.6.1",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "typescript": "^2.6.1",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  }
}
